public Paycheck()
{
    new string[128];
	for(new idle = 0; idle < MAX_PLAYERS; idle++)
	{
		if(IsPlayerConnected(idle))
		{
		    if(Account[idle][pJobTime] > 0)
		    {
		        Account[idle][pJobTime]--;
		    }
		    if(Account[idle][pBankSteal] > 0)
			{
			    Account[idle][pBankSteal]--;
			}
			if(Account[idle][pDonateRank] > 0)
			{
			    Account[idle][pPayDayHad] += 1;
			    if(Account[idle][pPayDayHad] >= 5)
			    {
			        Account[idle][pExp]++;
			        Account[idle][pPayDayHad] = 0;
			    }
			}
			if(RobbingBank > 0)
			{
				RobbingBank--;
			}
			Account[idle][pPayDay]++;
			new sss[20];
		    if(Account[idle][pPayDay] == 1)
			{
				format(sss, sizeof(sss), " ");
			}
			else
			{
				format(sss, sizeof(sss), "s ");
			}
			if(Account[idle][pCarLicS] > 0)
			{
			    Account[idle][pCarLicS]--;
			    if (Account[idle][pCarLicS] <= 0)
