dcmd_dance(playerid,params[])
{
	if(IsPlayerConnected(playerid))
	{
        if(!strlen(params)||!(strvalEx(params) < 4 && strvalEx(params) > 0))
        {
            SendClientMessage(playerid,LOL,"DEBUG: /dance [1 - 3]");
			return true;
        }
		if(IsPlayerInAnyVehicle(playerid))
		{
			return true;
		}
		if(MedicTime[playerid] == 1)
   		{
   			return true;
   		}
		if(strvalEx(params) == 1)
			{
				SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DANCE1);
                UsingAnim[playerid]=1;
		        
		        SendClientMessage(playerid,ORANGE,"DEBUG: /Stopanim to stop dancing.");
			}
			else if(strvalEx(params) == 2)
			{
            	SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DANCE2);
		        UsingAnim[playerid]=1;
		        SendClientMessage(playerid,ORANGE,"DEBUG: /Stopanim to stop dancing.");
			}
			else if(strvalEx(params) == 3)
			{
            	SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DANCE3);
		        UsingAnim[playerid]=1;
		        SendClientMessage(playerid,ORANGE,"DEBUG: /Stopanim to stop dancing.");
		  }
	}
	return true;
}

