public tazetimer(playerid)
{
	if(IsPlayerConnected(playerid))
	{
	    if(IsACop(playerid))
	    {
			nexttaze[playerid] = 0;
			SendClientMessage(playerid, COLOR_WHITE, "Now you can reload your Tazer Gun. (/reloadtazer)");
			return true;
		}
	}
	return true;
}

public OnVehicleDeath(vehicleid)
{
	SetTimerEx("StartVehicle",2000,0,"i",vehicleid);
	VehicleInfo[vehicleid][destroyed]++;
	SaveCar(vehicleid);
 	deadcar +=1;
}

public StartVehicle(vehicleid)
{
    DestroyVehicle(vehicleid);
 	AddStaticVehicleEx(VehicleInfo[vehicleid][cModel],VehicleInfo[vehicleid][cLocationx],VehicleInfo[vehicleid][cLocationy],VehicleInfo[vehicleid][cLocationz],VehicleInfo[vehicleid][cAngle],VehicleInfo[vehicleid][cColorOne],VehicleInfo[vehicleid][cColorOne],250000);
	VehicleInfo[vehicleid][cGas] = 100;
	ModVehicle(vehicleid);
	deadcar -=1;
}

