public LoadBans()
{
	new arrCoords[19][64];
	new strFromFile2[256];
	new File: file = fopen("bans.cfg", io_read);
	if (file)
	{
		new idx;
		while (idx < 4)
		{
			fread(file, strFromFile2);
			split(strFromFile2, arrCoords, '|');
			TimeBanInfo[idx][tbUsed] = strvalEx(arrCoords[0]);
			strmid(TimeBanInfo[idx][tbName], arrCoords[1], 0, strlen(arrCoords[1]), 255);
			strmid(TimeBanInfo[idx][tbIP], arrCoords[2], 0, strlen(arrCoords[2]), 255);
			TimeBanInfo[idx][tbTime] = strvalEx(arrCoords[3]);
			idx++;
		}
		fclose(file);
	}
	return true;
}

