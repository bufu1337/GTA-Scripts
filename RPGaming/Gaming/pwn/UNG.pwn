public Fillup()
{
	for(new i=0; i<MAX_PLAYERS; i++)
   	{
	   	if(IsPlayerConnected(i))
	   	{
		    new VID;
		    new string[256];
		    VID = GetPlayerVehicleID(i);
		    new price = FuelingLitre[i];
			if(Refueling[i] == 1)
		    {
			    if(VehicleInfo[VID][cFuelType] == 1)
			    {
				    price = FuelingLitre[i];
				    if(GetPlayerMoney(i) >= price)
					{
						VehicleInfo[VID][cGas] += FuelingLitre[i];
					    format(string,sizeof(string),"~w~Info: Vehicle filled up, for: ~g~$%d.",price);
					    TextDrawSetString(Textdrawz[i],string);
						TextDrawShowForPlayer(i,Textdrawz[i]);
						NoText[i]=5;
						GiveMoney(i, -price);
						Refueling[i] = 0;
						FuelingLitre[i] = 0;
						TogglePlayerControllable(i, true);
						SaveCar(VID);
						return true;
					}
					else
			  		{
				   	    format(string,sizeof(string),"ERROR: Not enough Money to refill, it costs $%d to fill your Vehicle.",price);
					    TextDrawSetString(Textdrawz[i],string);
						TextDrawShowForPlayer(i,Textdrawz[i]);
						Refueling[i] = 0;
						FuelingLitre[i] = 0;
						NoText[i]=5;
						TogglePlayerControllable(i, true);
						VehicleInfo[VID][cFuelType]=1;
						SaveCar(VID);
						return true;
			   		}
				}
				else if(VehicleInfo[VID][cFuelType] == 2)
				{
				    price = FuelingLitre[i]*2;
				    if(GetPlayerMoney(i) >= price)
					{
					VehicleInfo[VID][cGas] += FuelingLitre[i];
				    format(string,sizeof(string),"~w~Info: Vehicle filled up, for: ~g~$%d.",price);
				    TextDrawSetString(Textdrawz[i],string);
