public OnPlayerDisconnect(playerid, reason)
{
    new str[256], string[256];
    if(reason == 0) { format(string, sizeof(string), "%s (TimeOut)", Account[playerid][pName]); }
    if(reason == 1) { format(string, sizeof(string), "%s (Quit)", Account[playerid][pName]); }
    if(reason == 2) { format(string, sizeof(string), "%s (Kicked)", Account[playerid][pName]); }
    ProxDetector(20.0, playerid, string,COLOR_GRAD1,COLOR_GRAD2,COLOR_GRAD3,COLOR_GRAD4,COLOR_GRAD5);
    switch (reason)
	{
	    case 0:
	    {
		    format(str, 256, "*** %s Has Crashed ***", Account[playerid][pName]);
		    SendConnectMessage(str);
		    GetPlayerPos(playerid, CrashPos[playerid][0], CrashPos[playerid][1], CrashPos[playerid][2]);
			Account[playerid][pCrashed] = 1;
			Account[playerid][pInt] = GetPlayerInterior(playerid);
    	}
    	case 1:
    	{
		    format(str, 256, "*** %s Has Disconnected ***", Account[playerid][pName]);
		    SendConnectMessage(str);
			Account[playerid][pInt] = GetPlayerInterior(playerid);
			if(BankRob[playerid] > 0)
			{
			    GiveMoney(playerid, -BankRob[playerid]);
			}
	   	}
	 	case 2:
	 	{
	 	    if(BankRob[playerid] > 0)
			{
			    GiveMoney(playerid, -BankRob[playerid]);
			}
	 	}
	}
	GetPlayerPos(playerid, CrashPos[playerid][0], CrashPos[playerid][1], CrashPos[playerid][2]);
	Account[playerid][pInt] = GetPlayerInterior(playerid);
	Account[playerid][pCash] = GetPlayerMoney(playerid);
	OnlinePlayers--;
	if(VehicleInfo[GetPlayerVehicleID(playerid)][cType]==12)
	{
		SetVehicleToRespawn(GetPlayerVehicleID(playerid));
	}
	if(IsPlayerInAnyVehicle(playerid))
	{
	    Account[playerid][pQuitOnCar] =1;
	}
	OnPlayerUpdate(playerid);
	Goto[playerid] = 0;
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
	    if(IsPlayerConnected(i))
		{
	        if(TaxiAccepted[i] < 999)
	        {
		        if(TaxiAccepted[i] == playerid)
		        {
		            TaxiAccepted[i] = 999;
		            GameTextForPlayer(i, "~w~Taxi Caller~n~~r~Left the game", 5000, 1);
		            TaxiCallTime[i] = 0;
		            DisablePlayerCheckpoint(i);
		        }
	        }
	        else if(BusAccepted[i] < 999)
	        {
		        if(BusAccepted[i] == playerid)
		        {
		            BusAccepted[i] = 999;
		            GameTextForPlayer(i, "~w~Bus Caller~n~~r~Left the game", 5000, 1);
		            BusCallTime[i] = 0;
		            DisablePlayerCheckpoint(i);
		        }
	        }
	    }
	}
	if(TransportCost[playerid] > 0 && TransportDriver[playerid] < 999)
	{
	    if(IsPlayerConnected(TransportDriver[playerid]))
		{
		    new string1[64];
		    TransportMoney[TransportDriver[playerid]] += TransportCost[playerid];
		    TransportTime[TransportDriver[playerid]] = 0;
		    TransportCost[TransportDriver[playerid]] = 0;
		    format(string, sizeof(string1), "~w~Passenger left~n~~g~Earned $%d",TransportCost[playerid]);
		    GameTextForPlayer(TransportDriver[playerid], string1, 5000, 1);
		}
	}
	if(HireCar[playerid] != 999)
	{
		gCarLock[HireCar[playerid]] = 0;
		UnLockCar(HireCar[playerid]);
	}
	if(HireBike[playerid] != 999)
	{
		gCarLock[HireBike[playerid]] = 0;
		UnLockCar(HireBike[playerid]);
	}
	if(PlayerBoxing[playerid] > 0)
	{
	    if(Boxer1 == playerid)
	    {
	        if(IsPlayerConnected(Boxer2))
	        {
	        	PlayerBoxing[Boxer2] = 0;
	        	SetPlayerPos(Boxer2, 765.8433,3.2924,1000.7186);
	        	SetPlayerInterior(Boxer2, 5);
	        	Account[Boxer2][pInt] = 5;
	        	GameTextForPlayer(Boxer2, "~r~Match interupted", 5000, 1);
			}
	    }
	    else if(Boxer2 == playerid)
	    {
	        if(IsPlayerConnected(Boxer1))
	        {
	        	PlayerBoxing[Boxer1] = 0;
	        	SetPlayerPos(Boxer1, 765.8433,3.2924,1000.7186);
	        	SetPlayerInterior(Boxer1, 5);
	        	Account[Boxer2][pInt] = 5;
	        	GameTextForPlayer(Boxer1, "~r~Match interupted", 5000, 1);
			}
	    }
	    InRing = 0;
     	RoundStarted = 0;
		Boxer1 = 255;
		Boxer2 = 255;
		TBoxer = 255;
	}
    if(TransportDuty[playerid] == 1)
	{
		TaxiDrivers -= 1;
	}
    else if(TransportDuty[playerid] == 2)
	{
		BusDrivers -= 1;
	}
	else if(Account[playerid][pJob] == 7)
	{
	    if(JobDuty[playerid] == 1) { Mechanics -= 1; }
	}
	//CurrentMoney[playerid] = 0;
	BusrouteEast[playerid][0] = 0;
	BusrouteWest[playerid][0] = 0;
	return true;
}

