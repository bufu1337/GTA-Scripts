public HungryCheck()
{
	for(new j = 0; j<MAX_PLAYERS; j++)
	{
	    if(IsPlayerConnected(j))
	    {
	        if(PlayerDrunk[j] > 0) { PlayerDrunk[j] = 0; PlayerDrunkTime[j] = 0; GameTextForPlayer(j, "~p~Drunk effect~n~~w~Gone", 3500, 1); }
	        new Float:health;
	        if(Account[j][pHungry] <= 50)
	        {
	        	GetPlayerHealth(j,health);
	        	SetPlayerHealth(j,health-1);
	        }
	        if(Account[j][pHungry] <= 0)
	        {
	            Account[j][pHungry] = 0;
	        }
	        if(Account[j][pHungry] < 25)
	        {
	        	GetPlayerHealth(j,health);
	        	SetPlayerHealth(j,health-1);
				if(Hungry1[j]==0) { TextDrawHideForPlayer(j,Hunger1[j]); TextDrawShowForPlayer(j,Hunger1[j]); Hungry1[j]=1; }
            }
			if(STDPlayer[j]==1)
			{
			    GetPlayerHealth(j, health);
			    SetPlayerHealth(j, health - 5.0);
			}
			else if(STDPlayer[j]==2)
			{
			    GetPlayerHealth(j, health);
			    SetPlayerHealth(j, health - 12.0);
			    SendClientMessage(j, COLOR_LIGHTRED, "Lost 8 health due to STD.");
			}
			else if(STDPlayer[j]==3)
			{
			    GetPlayerHealth(j, health);
			    SetPlayerHealth(j, health - 20.0);
			    SendClientMessage(j, COLOR_LIGHTRED, "Lost 12 health due to STD.");
			}
		}
	}
	return true;
}

public CarRespawn(carid)
{
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
