public ValidateName(playerid)
{
	new name[MAX_PLAYER_NAME];
	GetPlayerName(playerid, name, sizeof(name));
	name[0] = toupper(name[0]); // Capitalize the first letter
	for(new i = 1; i < MAX_PLAYER_NAME; i++)
	{
	    if (name[i] == '_')
	    {
	        name[i+1] = toupper(name[i+1]); // Capitalize the other letters
	    }
	    else if (name[i] != '_' && name[i-1] != '_')
	    {
	        name[i] = tolower(name[i]);
	    }
	}
	printf("validated name: %s.", name);
	SetPlayerName(playerid, "New_Name");
	SetPlayerName(playerid, name);
}

