public Restarting()
{
	new i;
	new string[128];
	if(RestartTimer != DONT_RESTART)
	{
	//time to restart
		if(RestartTimer >= 60)
		{
			new Float:mins = floatdiv(float(RestartTimer),float(60));
			if((mins == 5.0) || (mins == 4.0) || (mins == 3.0) || (mins == 2.0))
			{
				format(string, sizeof(string), "Annoucement: Server Restarting in %d minutes.", floatround(mins));
				SendClientMessageToAll(COLOR_LIGHTRED, string);
				format(string, sizeof(string), "Annoucement: Finish what you are doing.");
				SendClientMessageToAll(COLOR_LIGHTRED, string);
			}
			if((mins == 1.0))
			{
			    format(string, sizeof(string), "Annoucement: Server Restarting in %d minute.", floatround(mins));
				SendClientMessageToAll(COLOR_LIGHTRED, string);
				format(string, sizeof(string), "Annoucement: Finish what you are doing.");
				SendClientMessageToAll(COLOR_LIGHTRED, string);
			}
		}
		else if((RestartTimer < 5) && (RestartTimer > 2))
		{
			format(string, sizeof(string), "Announcement: Server Restarting in %d seconds.", RestartTimer);
			SendClientMessageToAll(COLOR_LIGHTRED, string);
		}
		if(RestartTimer == 1)
		{
			SaveAccounts();
			SendClientMessageToAll(COLOR_LIGHTRED, "Server Restarting... Please rehash...");
			//restart time
			for(i=0;i< MAX_PLAYERS;i++)
			{
				Kick(i);
			}
			SendRconCommand("exit");
			RestartTimer--;
		}
		else if(RestartTimer == 0)
		{
		}
		else
		{
			RestartTimer--;
		}
	}
	return true;
}
