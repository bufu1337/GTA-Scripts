						SpawnPlayer(i);
						SetCameraBehindPlayer(i);
			        }
		    	    else
		  	      	{
						SpawnPlayer(i);
					}
					Spectate[i] = 255;
				}
				if(Spectate[i] == 254)
				{
					TogglePlayerControllable(i, true);
					SetPlayerInterior(i,Unspec[i][sPint]);
					Account[i][pInt] = Unspec[i][sPint];
					Account[i][pLocal] = Unspec[i][sLocal];
					SetPlayerPos(i, Unspec[i][sPx],  Unspec[i][sPy], Unspec[i][sPz]);
					Spectate[i] = 255;
				}
				if(Spectate[i] == 256)
				{
					SetPlayerToTeamColor(i);
					Spectate[i] = 255;
				}
				if(Spectate[i] == 257)
				{
					Spectate[i] = 254;
				}
			}
		}
	}
