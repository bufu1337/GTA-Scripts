public BackupClear(playerid, calledbytimer)
{
	if(IsPlayerConnected(playerid))
	{
		if(Account[playerid][pMember] == 1||Account[playerid][pLeader] == 1)
		{
			if (Account[playerid][pRequestingBackup] == 1)
			{
    			for(new i = 0; i < MAX_PLAYERS; i++)
				{
					if(IsPlayerConnected(i))
					{
						if(Account[i][pMember] == 1||Account[i][pLeader] == 1)
						{
						    if(OnDuty[playerid] == 0)
						    {
								SetPlayerMarkerForPlayer(i, playerid, TEAM_HIT_COLOR);
							}
							else{ SetPlayerColor(playerid, TEAM_BLUE_COLOR); }
						}
					}
				}
				if (calledbytimer != 1)
				{
					SendClientMessage(playerid, TEAM_BLUE_COLOR, "Your backup request has been cleared.");
				}
				else
				{
					SendClientMessage(playerid, TEAM_BLUE_COLOR, "Your backup request has been cleared automatically.");
				}
				Account[playerid][pRequestingBackup] = 0;
			}
			else
			{
				if (calledbytimer != 1)
				{
					SendClientMessage(playerid, COLOR_LIGHTRED, "ERROR: You don't have an active backup request!");
				}
			}
		}
		else
		{
			if (calledbytimer != 1)
			{
				SendClientMessage(playerid, COLOR_LIGHTRED, "ERROR: You are not a cop!");
			}
		}
	}
	return true;
}

