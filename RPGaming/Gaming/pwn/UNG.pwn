forward GangstaWalk(playerid);

public  GangstaWalk(playerid)
{
	new key, updown, leftright;
	GetPlayerKeys(playerid,key,updown,leftright);
	if(UsingAnim[playerid] == 1)
 	{
  		return true;
  	}
  	if (Walking[playerid] == 1)
	{
 	 	if ((key & KEY_WALK && updown & KEY_UP) || (key & KEY_WALK && updown & KEY_DOWN) || (key & KEY_WALK && leftright & KEY_LEFT) || (key & KEY_WALK && leftright & KEY_RIGHT))
		{
		   	KillTimer(walkingtime[playerid]);
			ApplyAnimation(playerid,"PED","WALK_gang1",4.1,1,1,1,1,1);
			walkingtime[playerid] = SetTimerEx("GangstaWalk",200,0,"d",playerid);
		}
		else ApplyAnimation(playerid,"PED","WALK_gang1",4.0,0,0,0,0,1), KillTimer(walkingtime[playerid]);
	}
	else if (Walking[playerid] == 2)
	{
 	 	if ((key & KEY_WALK && updown & KEY_UP) || (key & KEY_WALK && updown & KEY_DOWN) || (key & KEY_WALK && leftright & KEY_LEFT) || (key & KEY_WALK && leftright & KEY_RIGHT))
		{
		   	KillTimer(walkingtime[playerid]);
			ApplyAnimation(playerid,"PED","WALK_gang2",4.1,1,1,1,1,1);
			walkingtime[playerid] = SetTimerEx("GangstaWalk",200,0,"d",playerid);
		}
		else ApplyAnimation(playerid,"PED","WALK_gang2",4.0,0,0,0,0,1), KillTimer(walkingtime[playerid]);
	}
	return true;
}

