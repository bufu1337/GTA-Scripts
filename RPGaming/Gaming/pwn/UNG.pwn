public PayDay(playerid)
{
	new string[128];
	new account,checks,payz,level;
	{
		if(IsPlayerConnected(playerid))
		{
		    if(Account[playerid][pLevel] > 0)
		    {
				new playername2[MAX_PLAYER_NAME];
				GetPlayerName(playerid, playername2, sizeof(playername2));
				account = Account[playerid][pAccount];
				checks = Account[playerid][pPayCheck];
				payz = Account[playerid][pPayDay];
				level = Account[playerid][pLevel];
				new governpayday;
				if(Account[playerid][pDonateRank] >= 1)
				{
				    if(Account[playerid][pPayDay] >= 1) { Account[playerid][pAccount] = account+checks+200*payz+25*level; governpayday = 200*payz+25*level; }
				}
				else
				{
				    if(Account[playerid][pPayDay] >= 1) { Account[playerid][pAccount] = account+checks+100*payz+35*level; governpayday = 100*payz+35*level; }
				}
				GiveJobCheck(playerid);
				PlayerPlayMusic(playerid);
				SendClientMessage(playerid, COLOR_LIGHTBLUE, ".: BANK ACCOUNT CHECK :.");
				format(string, sizeof(string), "Job Paycheck: $%d   Tax Money: -$%d", checks, TaxValue);
				SendClientMessage(playerid, COLOR_WHITE, string);
				format(string, sizeof(string), "Government Paycheck: $%d", governpayday);
				SendClientMessage(playerid, COLOR_WHITE, string);
				format(string, sizeof(string), "Balance: $%d", account);
				SendClientMessage(playerid, COLOR_WHITE, string);
				SendClientMessage(playerid, COLOR_LIGHTBLUE, "|--------------------------------------|");
				Account[playerid][pPayCheck] = 0;
				Account[playerid][pHungry] -= 25;
				Account[playerid][pPayDay] = 0;
	 			format(string, sizeof(string), "New Balance: $%d", Account[playerid][pAccount]);
				SendClientMessage(playerid, COLOR_WHITE, string);
			}
		}
		OnPlayerUpdate(playerid);
	}
	return true;
}

public split(const strsrc[], strdest[][], delimiter)
{
	new i, li;
	new aNum;
	new len;
	while(i <= strlen(strsrc)){
	    if(strsrc[i]==delimiter || i==strlen(strsrc)){
	        len = strmid(strdest[aNum], strsrc, li, i, 128);
	        strdest[aNum][len] = 0;
	        li = i+1;
	        aNum++;
		}
		i++;
	}
	return true;
}

