public OnPlayerExitVehicle(playerid, vehicleid)
{
    TogglePlayerControllable(playerid, true);
	if(farmercheck[playerid] > 0)
	{
	    farmercheck[playerid]=0;
		DisablePlayerCheckpoint(playerid);
		TogglePlayerControllable(playerid, true);
	}
	if(StartedFlight[playerid] == 1)
	{
		SendFlightMessage(GetPlayerVehicleID(playerid), COLOR_YELLOW2, "The pilot has left the plane...");
    	TogglePlayerControllable(playerid, true);
    	StartedFlight[playerid] = 0;
    }
	if(InShamal[playerid] > 0)
	{
	    InShamal[playerid] = 0;
	}
 	if(Seatbelt[playerid] == 1)
 	{
 	    Seatbelt[playerid] = 0;
 	}
 	if(MeatDeliver[playerid]==1)
 	{
 	    MeatDeliver[playerid] = 0;
 	}
 	if(Trucker[playerid]==1)
 	{
 	    Trucker[playerid] = 0;
 	}
	if (VehicleInfo[vehicleid][cType] == 12)
	{
	    DMVTest[playerid] = 0;
	    DTA[playerid] = 0;
		SetVehicleToRespawn(vehicleid);
	}
 	if(farmercheck1[playerid] > 0)
	{
	    farmercheck1[playerid]=0;
		DisablePlayerCheckpoint(playerid);
		TogglePlayerControllable(playerid, true);
	}
	if(gPizzaBoy[playerid] > 0)
	{
	    DisablePlayerCheckpoint(playerid);
		GameTextForPlayer(playerid,"~r~Pizza boy mission failed~n~~y~To tough for you?!",5000,5);
		gPizzaTime[playerid] = 0;
		gPizzaBoy[playerid] = 0;
		gPizzaPoint[playerid] = 0;
		gPizzas[playerid] = 0;
	}
	if (BusrouteEast[playerid][0] != 0 || BusrouteWest[playerid][0] != 0)
	{
		BusrouteEnd(playerid, vehicleid);
		return true;
	}
	if (IsABus(vehicleid))
	{
		for (new i=0; i<=MAX_PLAYERS; i++)
		{
			if (IsPlayerConnected(i))
			{
				if (BusrouteEast[i][0] != 0 && BusrouteEast[i][1] == vehicleid)
				{
					//BusrouteEast[i][2] --;
					GameTextForPlayer(i, "~n~~n~~n~~n~~n~~n~~g~Passenger Left The Bus", 5000, 3);
				}
				else if (BusrouteWest[i][0] != 0 && BusrouteWest[i][1] == vehicleid)
				{
					//BusrouteWest[i][2] --;
					GameTextForPlayer(i, "~n~~n~~n~~n~~n~~n~~g~Passenger Left The Bus", 5000, 3);
				}
			}
		}
	}
	return true;
}

