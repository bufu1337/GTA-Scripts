public FoodTimer()
{
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		if(IsPlayerConnected(i))
		{
		    if(PlayerDrugging[i] != 0) // USING DRUGS DELAY
		    {
		        if(PlayerStoned[i] >= 4)
				{
			 		GameTextForPlayer(i, "~w~You overdosed!~n~~p~You feel sick.", 4000, 1);
			 		new Float:podhealth;
			 		GetPlayerHealth(i,podhealth);
			 		SetPlayerHealth(i,podhealth-25);
				}


			    new Float:health;
			    GetPlayerArmour(i, health);
			    if(Account[i][pDrugPerk] > 0)
			    {
			        new hp = 2 * Account[i][pDrugPerk]; hp += 15;
					SetPlayerArmour(i, health + hp);
			    }
			    else
			    {
			        SetPlayerArmour(i, health + 15.0);
			    }
			    SendClientMessage(i, COLOR_WHITE, "You finish using your drugs.");
			    Account[i][pDrugs] -= 2;
			    if(PlayerStoned[i] >= 4)
			    PlayerStoned[i] = 0;

				health = GetPlayerArmour(i, health);
				if (health > 100)
				SetPlayerArmour(i,100);
				PlayerDrugging[i] = 0;
		    }
		}
	}
}

public GetClosestCar(playerid)
{
     if (!IsPlayerConnected(playerid))
     {
          return -1;
     }
     new Float:prevdist = 100000.000;
     new prevcar;
     for (new carid = 0; carid < MAX_VEHICLES; carid++)
     {
          new Float:dist = GetDistanceToCar(playerid,carid);
          if ((dist < prevdist))
          {
               prevdist = dist;
               prevcar = carid;
          }
     }
     return prevcar;
}

public GetDistanceToCar(playerid,carid)
{
     new Float:dis;
     new Float:x1,Float:y1,Float:z1,Float:x2,Float:y2,Float:z2;
     if (!IsPlayerConnected(playerid)) {
     return -1;
     }
     GetPlayerPos(playerid,x1,y1,z1);
     GetVehiclePos(carid,x2,y2,z2);
     dis = floatsqroot(floatpower(floatabs(floatsub(x2,x1)),2)+floatpower(floatabs(floatsub(y2,y1)),2)+floatpower(floatabs(floatsub(z2,z1)),2));
     return floatround(dis);
}

public GetPlayerID(string[])
{
	for(new i = 0; i <= MAX_PLAYERS; i++)
	{
		if(IsPlayerConnected(i) == 1)
		{
			new testname[MAX_PLAYER_NAME];
			GetPlayerName(i, testname, sizeof(testname));
			if(strcmp(testname, string, true, strlen(string)) == 0)
			{
