					NoText[i]=5;
					GiveMoney(i, -price);
					Refueling[i] = 0;
					FuelingLitre[i] = 0;
					TogglePlayerControllable(i, true);
					SaveCar(VID);
					return true;
					}
					else
			  		{
				   	    format(string,sizeof(string),"ERROR: Not enough Money to refill, it costs $%d to fill your Vehicle.",price);
					    TextDrawSetString(Textdrawz[i],string);
						TextDrawShowForPlayer(i,Textdrawz[i]);
						NoText[i]=5;
						TogglePlayerControllable(i, true);
						Refueling[i] = 0;
						FuelingLitre[i] = 0;
						VehicleInfo[VID][cFuelType]=1;
						SaveCar(VID);
						return true;
			   		}
				}
				else if(VehicleInfo[VID][cFuelType] == 3)
				{
				    price = FuelingLitre[i]*3;
				    if(GetPlayerMoney(i) >= price)
					{
					VehicleInfo[VID][cGas] += FuelingLitre[i];
				    format(string,sizeof(string),"~w~Info: Vehicle filled up, for: ~g~$%d.",price);
				    TextDrawSetString(Textdrawz[i],string);
					TextDrawShowForPlayer(i,Textdrawz[i]);
