public OnFilterScriptInit()
{
	print("\n--------------------------------------");
	print(" Info script By [HiC]TheKiller Loaded   ");
	print("--------------------------------------\n");
	//Distance stuff
	if(!dini_Exists("Distances.ini"))
	{
		dini_Create("Distances.ini");
	}
	SetTimer("DistanceCount", 1000, true);
	return 1;
}

//Distance timer
forward DistanceCount();
public DistanceCount()
{
	for(new i=0;i<MAX_PLAYERS;i++)
	{
	    if(IsPlayerConnected(i))
	    {
        	TravelDist[i] = TravelDist[i] + GetPlayerSpeed(i);
		}
	}
	return 1;
}

public OnFilterScriptExit()
{
	print("\n--------------------------------------");
	print(" Info script By [HiC]TheKiller Unloaded ");
	print("--------------------------------------\n");
	return 1;
}

public OnPlayerConnect(playerid)
{
	//Distance Saving
	new Pname[24];
	GetPlayerName(playerid, Pname, 24);
	TravelDist[playerid] = dini_Int("Distances.ini", Pname);
	return 1;
}

public OnPlayerDisconnect(playerid, reason)
{
	//Distance Loading
    new Pname[24];
	GetPlayerName(playerid, Pname, 24);
	dini_IntSet("Distances.ini", Pname, TravelDist[playerid]);
	return 1;
}

public OnPlayerCommandText(playerid, cmdtext[])
{
	//Info Command
	dcmd(Info, 4, cmdtext);
	dcmd(Information, 11, cmdtext);
	return 0;
}
//The DCMD commands
dcmd_Info(playerid, params[])
{
	new id;
	if(!strlen(params)) return SendClientMessage(playerid, 0xFF0000AA, "Usage: \"/Info <playerid>\"");
	id = strval(params);
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, 0xFF0000AA, "Player Not connected!");
    Info(id, playerid);
	return 1;
}

dcmd_Information(playerid, params[])
{
	new id;
	if(!strlen(params)) return SendClientMessage(playerid, 0xFF0000AA, "Usage: \"/Information <playerid>\"");
	id = strval(params);
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, 0xFF0000AA, "Player Not connected!");
    Info(id, playerid);
	return 1;
}

